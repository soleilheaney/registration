# Environment variables for production deployment
# Copy this file to .env.production and fill in the values

# Prisma database connection string for PostgreSQL
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE?schema=SCHEMA
DATABASE_URL="postgresql://username:password@your-postgres-host.region.rds.amazonaws.com:5432/solstice?schema=public"

# Clerk Authentication
CLERK_PUBLISHABLE_KEY=${VITE_CLERK_PUBLISHABLE_KEY}
CLERK_SECRET_KEY=${CLERK_SECRET_KEY}
